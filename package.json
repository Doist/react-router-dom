{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-router-dom",
  "description": "A fork of react-router-dom v6.0.0-beta.0",
  "author": "Ernesto Garc√≠a <ernesto@doist.com>",
  "module": "dist/react-router-dom.esm.js",
  "size-limit": [
    {
      "path": "dist/react-router-dom.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-router-dom.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "husky": "^5.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "size-limit": "^4.10.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "history": "^5.0.0"
  }
